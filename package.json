{
  "name": "cloudberry",
  "version": "0.0.1",
  "author": "Azolver",
  "homepage": "https://azolver.com/",
  "scripts": {
    "ionic:build:after": "copyfiles --flat www/en-US/index.html www",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod:localize": "ng build --prod --localize && copyfiles --flat www/en-US/index.html www",
    "cp": "copyfiles --flat www/en-US/index.html www",
    "xi18n": "ng xi18n --out-file src/locale/messages.xlf",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --no-watch --no-progress --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "android": "ionic build --prod && copyfiles --flat www/en-US/index.html www\nrm -rf ./android\nionic cap add android\nnpm run replace:manifest\nionic cap sync android\nnpx capacitor-assets generate --android\nnpx cap run android",
    "android:dev-start": "ionic capacitor run android --livereload --external",
    "ios": "ionic build --prod\nrm -rf ./ios\nionic cap add ios\nnpm run replace:info\nionic cap sync ios\nnpx capacitor-assets generate\nnpx cap run ios",
    "android:build": "ionic build --prod\nnpx cap run android",
    "ios:build": "ionic build --prod\nnpx cap run ios",
    "android:open": "ionic cap open android",
    "ios:open": "ionic cap open ios",
    "replace:manifest": "cp ./TraxManifest.xml ./AndroidManifest.xml\nmv ./AndroidManifest.xml android/app/src/main/AndroidManifest.xml",
    "replace:info": "cp ./TraxInfo.plist ./Info.plist\nmv ./Info.plist ios/App/App/Info.plist",
    "install:python": "\nbrew install pyenv\nyes | pyenv install 3.10.3\npyenv global 3.10.3\necho \"export PATH=\\\"\\${HOME}/.pyenv/shims:\\${PATH}\\\"\" >> ~/.zshrc\nsource ~/.zshrc\nnpm install",
    "set-gplay-appId": "node bin/setGooglePlayPackageName.js",
    "set-version-name": "node bin/setVersionName.js",
    "config:hook:remove-install-package-permission": "node bin/addHookRemoveInstallPackagePermission.js",
    "config:hook:remove-external-storage-permission": "node bin/addHookRemoveExternalStoragePermission.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/localize": "^11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@auth0/angular-jwt": "5.0.1",
    "@bcyesil/capacitor-plugin-printer": "0.0.4",
    "@capacitor-community/file-opener": "^6.0.0",
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/assets": "^3.0.4",
    "@capacitor/camera": "^6.0.0",
    "@capacitor/clipboard": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/network": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic-native/android-permissions": "^5.27.0",
    "@ionic-native/core": "^5.0.7",
    "@ionic/angular": "5.3.1",
    "@ionic/storage": "^2.3.0",
    "angular2-signaturepad": "^2.11.0",
    "bootstrap": "^4.5.2",
    "build": "0.1.4",
    "capacitor-email-composer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "cordova": "~11.0.0",
    "ngx-barcode6": "1.0.14",
    "path-scurry": "^1.10.0",
    "pouchdb": "7.3.0",
    "pouchdb-upsert": "^2.2.0",
    "recursive-readdir": "^2.2.3",
    "rxjs": "~6.6.2",
    "scandit-capacitor-datacapture-barcode": "^6.25.2",
    "scandit-capacitor-datacapture-core": "^6.25.2",
    "sharp": "^0.28.2",
    "signature_pad": "^2.3.2",
    "subsink": "^1.0.1",
    "tslib": "^2.0.0",
    "web": "0.0.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/cli": "~11.2.18",
    "@angular/compiler": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@capacitor/cli": "^6.0.0",
    "@ionic/angular-toolkit": "^6.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "com-darryncampbell-cordova-plugin-intent": "^2.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.2",
    "typescript": "~4.1.6",
    "xml2js": "^0.4.23"
  },
  "description": "Azolver Tracking Solution",
  "cordova": {
    "plugins": {
      "com-darryncampbell-cordova-plugin-intent": {}
    }
  }
}
